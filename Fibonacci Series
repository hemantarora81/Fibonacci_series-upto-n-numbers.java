package com.company;
import java.util.Scanner;
//Question 3 Write a Java program to calculate Fibonacci Series up to n numbers.
public class fibronacci_series {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter number");
        int n = sc.nextInt();
        //int n = 100;
        int t1 = 0;
        int t2 = 1;
        System.out.print("Upto To" + ":");
        while(t1<= n){
            System.out.print(t1 +" +");
            int sum = t1+t2;
            t1=t2;
            t2 = sum;
        }
    }
}
